#include <stdio>
#include <stdlib>
#include <math.h>

int f1(int n)
{
    int a[n];
    int i=0;
    int j=6;
    int q=j*j;
    int somma=0;
    for (i=0;i<n;i++){
        somma=somma+q;
        j+=6;
        q=j*j;
    }
    double ris=sqrt(somma);
    printf("quadrato = %.2f\n",ris);
    return ris;
}
